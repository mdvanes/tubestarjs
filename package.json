{
  "name": "tubestarjs",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev:tsc": "tsc -w -p src/tsconfig.json",
    "dev:vite": "vite",
    "dev:node": "nodemon index.ts",
    "start": "npm run build:assets && concurrently --kill-others \"npm:dev:node\" \"npm:dev:tsc\" \"npm:dev:vite\"",
    "build:assets": "cp assets/*.png dist/assets",
    "build": "tsc -p src/tsconfig.json && vite build && npm run build:assets",
    "serve": "ts-node index.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "echarts": "^5.4.2",
    "express": "^4.18.2",
    "leaflet": "^1.9.3",
    "ts-node": "^10.9.1",
    "xml-js": "^1.6.11",
    "zeromq": "^5.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/leaflet": "^1.9.3",
    "@types/zeromq": "^5.2.2",
    "concurrently": "^8.0.1",
    "nodemon": "^2.0.22",
    "vite": "^4.2.1"
  }
}
